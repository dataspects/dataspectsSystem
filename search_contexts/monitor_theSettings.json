{
  "esQuery": {
    "index": "",
    "body": {
      "_source": {
        "includes": [
          "HasEntityURL",
          "HasEntityType",
          "HasEntityTitle",
          "HasEntityBlurbTEXT",
          "HasEntityBlurbHTML",
          "HasEntityKeywords",
          "HasEntityAnnotations",
          "HasEntityContentTEXT",
          "HasEntityContentHTML"
        ],
        "excludes": []
      },
      "version": true,
      "size": 3,
      "query": {
        "bool": {
          "should": [
            {
              "query_string": {
                "default_field": "HasEntityName",
                "query": "{{query.params.queryString}}",
                "boost": "10"
              }
            },
            {
              "query_string": {
                "default_field": "HasEntityContentTEXT",
                "query": "{{query.params.queryString}}"
              }
            }
          ]
        }
      },
      "highlight" : {
        "fields" : {
          "HasEntityName" : {},
          "HasEntityContentTEXT" : {}
        }
      },
      "explain": false,
      "profile": false
    }
  },
  "searchResultSpecials": [
    {
      "conditions": {
        "must": {
          "HasEntityType": "Feature"
        }
      },
      "actions": [
        {
          "COMMENT": "",
          "title": "Implemented by",
          "query": {
            "queryName": "must_match_predicate_and_match_object",
            "params": {
              "predicate": "ImplementsFeature",
              "object": ".search-result@@@hasentityname"
            },
            "return": "subject"
          },
          "view": "search/result_classes/features_feature_class",
          "show_button": {
            "conditions": {
              "always": true
            }
          },
          "execute": true
        }
      ]
    }
  ]
}
